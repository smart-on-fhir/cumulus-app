{
  "name": "cumulus-app",
  "version": "2.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "bcryptjs": "^2.4.3",
    "cli-color": "^2.0.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dockerode": "^3.3.1",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "express": "^4.17.2",
    "express-validator": "^6.14.2",
    "form-data": "^4.0.0",
    "google-map-react": "^2.1.10",
    "highcharts": "^9.3.2",
    "html2canvas": "^1.4.1",
    "if-env": "^1.0.4",
    "mailgun.js": "^8.0.2",
    "moment": "^2.29.1",
    "pg": "^8.7.3",
    "react": "^17.0.2",
    "react-contenteditable": "^3.3.6",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.3",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "run-container": "^1.2.3",
    "sass": "^1.45.1",
    "sequelize": "^6.16.0",
    "slug": "^5.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.4",
    "umzug": "^3.1.1",
    "web-vitals": "^2.1.2",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "scripts": {
    "configure": "ts-node --skipProject --transpile-only configure.ts",
    "build": "react-scripts build",
    "start": "npm run start:manual",
    "start:docker": "if-env NODE_ENV=production && npm run start:prod:docker || npm run start:dev:docker",
    "start:manual": "if-env NODE_ENV=production && npm run start:prod:manual || npm run start:dev:manual",
    "start:prod:docker": "cd docker && docker-compose --env-file='../.env.production.local' up",
    "start:dev:docker": "cd docker && docker-compose --env-file='../.env.development.local' up",
    "start:prod:manual": "ts-node --skipProject --transpile-only -r dotenv-expand/config backend dotenv_config_path=.env.production.local",
    "start:dev:manual": "nodemon -e js,ts,json -w backend -x 'ts-node --skipProject --transpile-only -r dotenv-expand/config backend dotenv_config_path=.env.development.local'",
    "start:hmr": "PORT=$HMR_PORT react-scripts start dotenv_config_path=.env.development.local",
    "eject": "react-scripts eject",
    "test": "DOTENV_CONFIG_PATH=.env.test nyc --reporter=lcov ts-mocha -p test/tsconfig.json",
    "test:frontend": "react-scripts test",
    "test:server:watch": "nodemon -w backend -e ts,json -x 'npm test'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.3.4",
    "@types/cli-color": "^2.0.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/google-map-react": "^2.1.4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "@types/pg": "^8.6.5",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/sequelize": "^4.28.14",
    "@types/slug": "^5.0.3",
    "chai": "^4.3.7",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "ts-mocha": "^10.0.0"
  },
  "proxy": "http://127.0.0.1:4001",
  "engines": {
    "node": "18"
  }
}
